{
  "name": "@deltecho/shared",
  "version": "1.0.0",
  "description": "Shared types, utilities, and constants for all deltecho packages",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants.js",
      "types": "./dist/constants.d.ts"
    },
    "./logger": {
      "import": "./dist/logger.js",
      "types": "./dist/logger.d.ts"
    },
    "./util": {
      "import": "./dist/util.js",
      "types": "./dist/util.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "deltecho",
    "shared",
    "types",
    "utilities"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
